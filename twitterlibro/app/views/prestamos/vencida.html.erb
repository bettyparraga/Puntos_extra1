   <th>Libro</th>
   <th>Fecha inicio prestamo</th>
   <th>Fecha fin prestamo</th>
   <th></th>
 </tr>

<% @prestamos.each do |prestamo| %>
 <tr>
   <td><%= prestamo.cliente.nombre %></td>
   <td><%= prestamo.libro.titulo %></td>
   <td><%= prestamo.fi_prestamo %></td>
   <td><%= prestamo.ff_prestamo %></td>
   <td><%= link_to 'Enviar Tweet', enviar_mensaje_prestamo_path(prestamo) %></td>
 </tr>
<% end %>
</table>

